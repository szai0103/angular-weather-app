{
  "name": "angular-weather-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "release": "standard-version",
    "test": "ng test",
    "test:coverage": "ng test --code-coverage",
    "e2e": "ng e2e",
    "lint": "ng lint --format json > lintingReport.json",
    "precommit": "npm run lint",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run-record": "cypress run --record --key 3714cfc6-2b13-4ba9-b464-3d742bf184c8",
    "cypress:coverage-summary": "npx nyc report --reporter=text-summary --exclude \"**/*.*spec.ts\"",
    "cypress:coverage-text": "nyc report --reporter=lcov --reporter=text --exclude \"**/*.*spec.ts\"",
    "cypress:lcov": "nyc report --reporter=lcov --reporter=text"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.0",
    "@angular/common": "~10.2.0",
    "@angular/compiler": "~10.2.0",
    "@angular/core": "~10.2.0",
    "@angular/forms": "~10.2.0",
    "@angular/platform-browser": "~10.2.0",
    "@angular/platform-browser-dynamic": "~10.2.0",
    "@angular/router": "~10.2.0",
    "@ng-bootstrap/ng-bootstrap": "^7.0.0",
    "@scu/core-ui": "^3.3.0",
    "auth0-js": "latest",
    "bootstrap": "^4.5.2",
    "eslint-config-airbnb-angular": "^1.0.0",
    "rxjs": "~6.6.0",
    "snyk": "^1.421.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.0",
    "@angular/language-service": "~10.2.0",
    "@briebug/cypress-schematic": "^4.2.4",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^11.0.0",
    "@cypress/code-coverage": "^3.8.1",
    "@cypress/webpack-preprocessor": "^5.4.6",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "angular2-template-loader": "0.6.2",
    "babel-plugin-istanbul": "^6.0.0",
    "codelyzer": "^6.0.0",
    "commitizen": "^4.1.2",
    "core-js": "3.6.5",
    "cypress": "^5.5.0",
    "cypress-angular-unit-test": "^3.3.1",
    "cypress-image-snapshot": "3.1.1",
    "cz-conventional-changelog": "^3.2.0",
    "html-loader": "1.1.0",
    "husky": "^4.3.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "istanbul-lib-coverage": "^3.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-lint-report": "^0.1.11",
    "ngx-build-plus": "^10.1.1",
    "ngx-semantic-version": "^2.2.0",
    "nyc": "^15.1.0",
    "semantic-release": "^17.1.2",
    "source-map-support": "^0.5.19",
    "standard-version": "^9.0.0",
    "to-string-loader": "1.1.6",
    "ts-loader": "^8.0.4",
    "ts-node": "^8.3.0",
    "tslint": "~6.1.0",
    "typescript": "^4.0.5"
  },
  "nyc": {
    "report-dir": "coverage/integration",
    "reporter": [
      "text",
      "json",
      "lcov"
    ],
    "all": true,
    "include": [
      "src/app/**/*.ts"
    ],
    "exclude": [
      "src/app/models",
      "src/main.ts",
      "src/polyfills.ts",
      "src/test.ts"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/szai0103/angular-weather-app.git"
  }
}
